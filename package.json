{
  "name": "@ghlandy/storage",
  "version": "1.1.1",
  "description": "Browser localStorage/sessionStorage helper",
  "main": "dist/cjs.js",
  "module": "dist/esm.js",
  "types": "types",
  "files": [
    "dist",
    "types"
  ],
  "scripts": {
    "prebuild": "rimraf dist types",
    "prepublishOnly": "npm run build",
    "build": "rollup -c",
    "prettier": "prettier --write \"**/*.{js,ts,md}\"",
    "lint": "eslint --fix \"**/*.{js,ts}\"",
    "test": "jest",
    "cov": "jest --coverage --verbose=false",
    "codecov": "codecov"
  },
  "keywords": [
    "storage",
    "localStorage",
    "sessionStorage"
  ],
  "homepage": "http://storage.ghlandy.com/",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GHLandy/ghlandy-storage.git"
  },
  "bugs": "https://github.com/GHLandy/ghlandy-storage/issues",
  "author": "GHLandy <ghlandy@ghlandy.com>",
  "license": "MIT",
  "dependencies": {},
  "devDependencies": {
    "@babel/core": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "@types/jest": "^24.0.18",
    "@typescript-eslint/eslint-plugin": "^2.28.0",
    "@typescript-eslint/parser": "^2.28.0",
    "babel-jest": "^24.9.0",
    "codecov": "^3.5.0",
    "eslint": "^6.8.0",
    "husky": "^3.0.4",
    "jest": "^24.9.0",
    "lint-staged": "^9.2.3",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.2",
    "rollup": "^1.20.3",
    "rollup-plugin-typescript2": "^0.27.0",
    "ts-jest": "^24.1.0",
    "typescript": "^3.6.2"
  },
  "lint-staged": {
    "**/*.{js,ts,md}": [
      "prettier --write",
      "git add"
    ],
    "**/*.{js,ts}": [
      "eslint --fix",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  }
}
